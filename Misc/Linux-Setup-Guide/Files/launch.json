{
    "version": "0.2.0",
    "configurations": [
        {
			"name": "Debug - Android",
			"type": "mono",
			"preLaunchTask": "run-debug-android",
			"request": "attach",
      			"address": "localhost",
      			"port": 10000
		},
        {
			"name": "Attach - Android",
			"type": "mono",
			"request": "attach",
			"address": "127.0.0.1",
			"port": 10000
		},
        {
            "name": "Launch with Wine (Debug)",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "wine",
            "args": [
                "${workspaceFolder}/bin/Debug/net9.0-windows7.0/win-x64/gip.iplus.client.exe"
            ],
            "cwd": "${workspaceFolder}/bin/Debug/net9.0-windows7.0/win-x64",
            "preLaunchTask": "Build Windows (wine)",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch with Wine (Release)",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "/usr/bin/wine",
            "args": [
                "${workspaceFolder}/bin/Release/net9.0-windows7.0/win-x64/gip.iplus.client.exe"
            ],
            "cwd": "${workspaceFolder}/bin/Release/net9.0-windows7.0/win-x64",
            "preLaunchTask": "Build Windows Release (wine)",
            "console": "integratedTerminal"
        }                
    ]
}
